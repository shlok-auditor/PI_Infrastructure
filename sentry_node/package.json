{
  "name": "jwt_auth_node_typeorm",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/main.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev-run": "yarn build && yarn dev",
    "dev-local": "yarn build && yarn local",
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/main.ts",
    "build": "rimraf dist && ./node_modules/.bin/tsc -p tsconfig.json",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrations": "./node_modules/.bin/ts-node ./node_modules/typeorm/cli.js migration:run -d src/utilities/orm.ts",
    "dev": "./node_modules/.bin/env-cmd -f .env node ./dist/main.js",
    "local": "./node_modules/.bin/env-cmd -f .env.local node ./dist/main.js",
    "create-db-dev": "./node_modules/.bin/env-cmd -f .env ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -d src/utils/data-source.ts -s no",
    "create-db-local": "./node_modules/.bin/env-cmd -f .env.local ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:create -d src/utils/data-source.ts -s no",
    "drop-db-dev": "./node_modules/.bin/env-cmd -f .env ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop -d src/utils/data-source.ts",
    "drop-db-local": "./node_modules/.bin/env-cmd -f .env.local ts-node ./node_modules/typeorm-extension/dist/cli/index.js db:drop -d src/utils/data-source.ts"
  },
  "dependencies": {
    "@sentry/node": "^7.57.0",
    "@types/swagger-stats": "^0.95.8",
    "@types/xlsx": "^0.0.36",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "env-cmd": "^10.1.0",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "grafana": "^1.0.1",
    "html-to-text": "^8.2.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "multer": "^1.4.4",
    "node-geocoder": "^4.2.0",
    "nodemailer": "^6.7.5",
    "pg": "^8.4.0",
    "pug": "^3.0.2",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "sharp": "^0.30.4",
    "swagger-parser": "^10.0.3",
    "swagger-stats": "^0.99.5",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.17",
    "typeorm-extension": "^2.6.2",
    "typeorm-migrations-plugin": "^0.9.1",
    "uuid": "^9.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.41",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/html-to-text": "^8.1.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.11.10",
    "@types/nodemailer": "^6.4.4",
    "@types/pug": "^2.0.6",
    "@types/sharp": "^0.30.2",
    "morgan": "^1.10.0",
    "ts-node": "10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "4.5.2"
  }
}
